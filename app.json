{
  "expo": {
    "name": "MovieDeep",
    "slug": "MovieDeep",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/app-icon.png",
    "splash": {
      "image": "./assets/icons/splash.png",
      "backgroundColor": "#030014"
    },
    "scheme": "movies",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/app-icon.png",
        "backgroundColor": "#030014"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.abdullah1q1q.MovieDeep"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/app-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash.png",
          "backgroundColor": "#030014"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b1344805-171f-44db-9c5e-369d437247c8"
      }
    },
    "owner": "abdullah1q1q"
  }
}
